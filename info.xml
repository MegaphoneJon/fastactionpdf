<?xml version="1.0"?>
<extension key="fastactionpdf" type="module">
  <file>fastactionpdf</file>
  <name>Fast Action PDFs</name>
  <description>Use the Fast Action Links and the PDF API extensions together to generate PDFs via action links.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jon Goldberg</author>
    <email>jon@megaphonetech.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/MegaphoneJon/fastactionpdf</url>
    <url desc="Documentation">https://github.com/MegaphoneJon/fastactionpdf/blob/master/README.md</url>
    <url desc="Support">https://github.com/MegaphoneJon/fastactionpdf/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-07-08</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.31</ver>
  </compatibility>
  <requires>
    <ext>com.megaphonetech.fastactionlinks</ext>
    <ext>org.civicoop.pdfapi</ext>
  </requires>
  <comments/>
  <civix>
    <namespace>CRM/Fastactionpdf</namespace>
    <format>22.05.0</format>
  </civix>
</extension>
